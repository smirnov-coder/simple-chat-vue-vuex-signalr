{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SqliteConnection": "Data Source={DataDirectory}\\SimpleChat.db"
  },
  "Authentication": {
    "VKontakte": {
      "ClientId": "YOUR_VKONTAKTE_CLIENT_ID_HERE",
      "ClientSecret": "YOUR_VKONTAKTE_CLIENT_SECRET_HERE"
    },
    "Facebook": {
      "AppId": "YOUR_FACEBOOK_APP_ID_HERE",
      "AppSecret": "YOUR_FACEBOOK_APP_SECRET_HERE"
    },
    "Odnoklassniki": {
      "ApplicationId": "YOUR_ODNOKLASSNIKI_APPLICATION_ID_HERE",
      "ApplicationSecretKey": "YOUR_ODNOKLASSNIKI_APPLICATION_SECRET_KEY_HERE",
      "ApplicationKey": "YOUR_ODNOKLASSNIKI_APPLICATION_KEY_HERE"
    }
  },
  "Email": {
    "Address": "YOUR_EMAIL_ADDRESS_HERE",
    "Port": "YOUR_EMAIL_PORT_HERE",
    "UserName": "YOUR_EMIAL_USERNAME_HERE",
    "Password": "YOUR_EMAIL_PASSWORD_HERE"
  }
}
